{
  "name": "readbox",
  "version": "1.0.1",
  "main": "lib/readbox.js",
  "description": "dropbox read directory or file",
  "author": "",
  "homepage": "http://github.com/coderaiser/readbox",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/readbox.git"
  },
  "scripts": {
    "test": "tape 'test/*.js'",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "watch:test": "npm run watcher -- npm test",
    "watcher": "nodemon -w test -w lib --exec",
    "lint": "eslint lib test",
    "build": "babel lib -d legacy",
    "wisdom": "redrun build legacy",
    "legacy": "echo \"module.exports = require('./readbox')\"> legacy/index.js"
  },
  "dependencies": {
    "currify": "^2.0.6",
    "dropbox-stream": "^2.0.0",
    "dropboxify": "^2.0.0",
    "es6-promisify": "^6.0.0",
    "squad": "^3.0.0",
    "string-to-stream": "^1.1.0",
    "try-to-catch": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "clear-module": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.0.0",
    "fast-async": "^7.0.4",
    "mock-require": "^3.0.1",
    "nodemon": "^1.14.11",
    "nyc": "^13.0.1",
    "pullout": "^2.0.0",
    "redrun": "^7.0.0",
    "sinon": "^6.0.0",
    "sinon-called-with-diff": "^2.0.0",
    "tape": "^4.6.3"
  },
  "keywords": [
    "directory",
    "read",
    "readdir",
    "readify",
    "file",
    "folder",
    "dropbox"
  ],
  "engines": {
    "node": ">=8"
  },
  "license": "MIT"
}
